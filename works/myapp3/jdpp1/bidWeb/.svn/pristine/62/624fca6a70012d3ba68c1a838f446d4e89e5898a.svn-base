package com.eachnet.bid.web.item.constants;

/**
 * 商品状态
 * Created by carlos on 18-1-29.
 */
public enum ItemStatus {

    UnShelve(0 , "下架"),
    Shelve(1 , "上架"),
    Trailer(2 , "预告中"),
    Bidding(3 , "竞价中"),
    BiddingSuccess(4 , "竞价成功"),
    AuctionInvalid(5 , "流拍"),;


    private Integer value;
    private String desc;


    ItemStatus(Integer value , String desc){
        this.value = value;
        this.desc = desc;
    }

    public Integer getValue() {
        return value;
    }

    public void setValue(Integer value) {
        this.value = value;
    }

    public String getDesc() {
        return desc;
    }

    public void setDesc(String desc) {
        this.desc = desc;
    }
}

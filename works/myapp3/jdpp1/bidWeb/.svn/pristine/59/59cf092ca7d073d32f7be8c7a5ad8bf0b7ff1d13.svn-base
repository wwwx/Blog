<script>
// ctrl+F2显示奖金 5秒后消失
var isTrue = true ;
function doHide(){
	$('.rTab').hide();
	isTrue=true;
}

$(document).ready(function() {
	
	$(document).keydown(function(e) {
		if (e.ctrlKey) {
			if(e.which == 113 && isTrue ){
				doToggle();
			}
		}
	});
	function doToggle(){
		var obj = this ; 
		$(".rTab").toggle();
		isTrue = false ;
		setTimeout(function(){
			obj.doHide();
		},5000);//1000为1秒钟
	}
	
});

function getFileSize(input){
	var objValue = input.value;
	if (objValue=="") return 0;
	var fileSize = 0;
	try {
		fileSize = parseInt(input.files[0].size);
	} catch (e){
	}
	return fileSize;
}
function uploadFile(id){
	//判断文件大小
	var maxSize = 5*1024*1024;
	var $input = $("#"+id);
	var fileInput = $input[0];
	var byteSize = getFileSize(fileInput);
	var $prompt = $('.prompt_photo');
	if(byteSize > maxSize) {
		showMsg($prompt, '图片大小请控制在5M以内');
		return;
	}
	//判断是否有选择上传文件
	var imgPath = $input.val();
	if(!imgPath) {
		showMsg($prompt, '仅支持jpg、png格式的图片');
		return;
	}
	//判断上传文件的后缀名
	var strExtension = imgPath.substr(imgPath.lastIndexOf('.') + 1);
	if(strExtension != 'jpg' && strExtension != 'png') {
		showMsg($prompt, '仅支持jpg、png格式的图片');
		return;
	}
	$('#UploadIdentityForm').prepend(fileInput);
	var formTarget = $('#UploadIdentityFormTarget');
	var iframe = formTarget[0];
	uploadSuccessID = id;
	if (iframe.attachEvent){
		iframe.detachEvent("onload", uploadSuccess);
		iframe.attachEvent("onload", uploadSuccess);
	} else {
		iframe.onload = function(){
			uploadSuccess();
		};
	}
	$('#UploadIdentityForm')[0].submit();
	$('.iOrfilings .pItem.front .select_photo').append($('#frontFile'));
	$('.iOrfilings .pItem.back .select_photo').append($('#backFile'));
}
var uploadSuccessID = '';
function uploadSuccess(){
	var id = uploadSuccessID;
	var content = '';
	try{ content = $('body', window.frames["UploadIdentityFormTarget"].document).html(); }
	catch(err1){ }
	var result = null;
	if((typeof JSON) === 'undefined' || (typeof JSON.parse) === 'undefined') result = eval("(" + content+ ")");
	else result = JSON.parse(content);
	if(result) {
		if(result.returnCode === '70000') {
			var key = id.replace('File', '');
			var span = $('.pItem.' + key);
			$('.select_photo img', span).remove();
			$('.select_photo', span).prepend('<img src="' + result.returnResult + '" width="98px" height="58px"/>');
			$('.hidde_'+id).val(result.returnResult);
			showMsg($('.prompt_photo'));
		}
	}
}
function showMsg($prompt, msg) {
	if (msg) {
		$prompt.addClass("error").removeClass("success").html(msg);
	} else {
		$prompt.addClass("success").removeClass("error").html("");
	}
	return !msg;
}
function iSubmit() {
	var $elem = $("#util-dialog-new"), data = {};
	$("input", $elem).trigger("blur");
	if ($('.error:visible', $elem).length > 0)
		return;
	$("input[type='text'],input[class='checkFileInfo']").each(function() {
		this.id && (data[this.id] = this.value);
	});
	$.ajax({
		type: "post",
		url: "/myShoppingOrderWeb/sfCheck.do",
		data:  $("#sfCheckFrom").serialize(),
		dataType: "json",
		success : function(data) {
			if(data!=null){
				if(data.reCode){
					JEND.page.popbox.close();
					setTimeout(function(){ JEND.page.showLoading('身份信息修改成功'); }, 500);
					//刷新当前页面
					location.reload();
				}else{
					alert(data.msg);
				}
			}
		}
	});
}
$(".iOrTip .iOBtn").click(showSfzDiv);

function showSfzDiv() {
	JEND.page.popbox({ id : 'util-dialog-new' });
	$("#usrNum").val($("#usrNumHid").val());
	$('#UploadIdentityForm').attr('action','//my.'+ JEND.server.uleUrl +'/usr/identity/upload.do');
	showMsg($('.prompt'));
	$('.checkFileInfo').val('');
	var $pItem = $('.iOrfilings .pItem');
	$('.select_photo img', $pItem).remove();
	$('.select_photo input', $pItem).val('');
	$('.select_photo', $pItem).unbind('click');
	$('.select_photo', $pItem).click(function(e){
		$('.iOrfilings .pItem.front .select_photo').append($('#frontFile'));
		$('.iOrfilings .pItem.back .select_photo').append($('#backFile'));
		$('.select_photo input', $pItem).unbind('change');
		$('input', this).change(function(){
			uploadFile(this.id);
		});
	});
}


$("#usrNum").blur(function() {
	var $prompt = $(this).parent().siblings(".prompt");
	var reg = /(^\d{15}$)|(^\d{17}([0-9]|X)$)/;
	var val = this.value.trim(), msg = "";
	if (!val) {
		msg = "请输入身份证号！";
	} else if (!reg.test(val)) {
		msg = "身份证格式有不正确,请重新输入！";
	}
	showMsg($prompt, msg);
});
</script>

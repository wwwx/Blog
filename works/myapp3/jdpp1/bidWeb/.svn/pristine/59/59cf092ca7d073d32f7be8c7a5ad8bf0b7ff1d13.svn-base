<script language="javascript" type="text/javascript">

$(document).ready(function() {
	/**
	 * 取消订单
	 * @return {[type]} [description]
	 */
	$(".cancel").click(function(ev){
	  /**
	   * dorderid 订单类型
	   * undefined 大订单 false
	   * string 小订单
	   */
	  var bookType = $(this).attr('dorderid') || false,

	  /**
	   * 取消订单类型 data-type
	   * 0: 有退款理由的弹出层
	   * 3: 无退款理由的弹出层
	   * 1: 只缺货取消
	   * 2: 缺货和整单取消
	   */
	      canType = $(ev.target).attr('data-type'),
	      escId = $(ev.target).attr('data-id');
	  // 根据订单号发送ajax判断是否超过5分钟
	  var urlStr= "/myShoppingOrderWeb/beforeCancelOrder.do";
		dataStr="escOrderid=" + escId;
		var returnObj = $.ajax({
			type: "POST",
			url: urlStr,
			data:dataStr,
			cache: false,
			async: false
	  });
		var returnStr = returnObj.responseText ;
		var jsonObj =  returnStr.parseJSON(); 
	  //JEND.util.dialog.alert(returnStr);
	  //setHasReason(bookType, escId, 1);
	  	var retCode = jsonObj.retCode ;
		var retMsg = jsonObj.retMsg ;
		if("0000" == retCode || "415" == retCode || "416" == retCode || "417" == retCode){
			window.open("/myShoppingOrderWeb/toCancelOrder.do?escOrderid="+escId+"&timestamp="+new Date().getTime()) ;
			return ;
		}
		if("0002" == retCode){
			JEND.util.dialog.alert(retMsg);
			return ;
		}
		if("0001" == retCode){
	    	switch(canType){
		      case '1':
		        onlyLackReason(bookType, escId, 2);
		        break;
		      case '2':
		        setlackReason(bookType, escId, 2);
		        break;
		      case '3':
		        setNoReason(bookType, escId, 1);
		        break;
		      default:
		        setHasReason(bookType, escId, 1);
		        break;
		    }
	    }
	});
});





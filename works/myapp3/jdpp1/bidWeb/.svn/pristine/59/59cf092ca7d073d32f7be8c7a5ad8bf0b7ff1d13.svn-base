        document.getElementById("cancelReason").value = reason;
        document.getElementById("operateType").value = operateType;//用于判断是整单取消还是缺货物品取消
        //setTimeout(function(){ JEND.page.showLoading('正在取消订单……'); }, 500);
        JEND.page.showLoading('正在取消订单……');
        $.post("/myShoppingOrderWeb/cancelBuyerOrder.do?timestamp=" + new Date().getTime(), $("#orderDetail").serialize(), function(data) {
					//mask2.hide();

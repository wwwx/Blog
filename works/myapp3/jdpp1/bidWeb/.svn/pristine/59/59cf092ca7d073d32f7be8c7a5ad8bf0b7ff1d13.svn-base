     function resendActiveMsg(escId,obj){
        $(obj).attr("disabled",true);
        if(escId==""){
  			JEND.util.dialog.alert('活动太火爆啦，请稍后再试'); 
  			return;
  		}
  		JEND.util.dialog.confirm({title:'操作确认', message:'您是否确定要激活？',
			    callback:function(){
		    	var urlStr="/myShoppingOrderWeb/resendActiveOrderMsg.do";
				dataStr="escOrderid=" + escId;
				var returnStr = $.ajax({
					type: "POST",
		    		url: urlStr,
		    		data:dataStr,
		    		cache: false,
		    		async: false
		        }).responseText;
		        JEND.util.dialog.alert(returnStr);
		        if(returnStr.indexOf("重发成功") > -1){
		         	$(obj).css("color", "gray").removeAttr("onclick");;
		        }
	      		 }
			});
    }
     
  	function confirmGotoPay(orderAmountStatus,escId,type){
 		if(orderAmountStatus=='3'){
			JEND.util.dialog.alert('请先付完定金后再付尾款!'); 
			return;
		}
 		if(escId=='' || type==''){
			JEND.util.dialog.alert('活动太火爆啦，请稍后再试!'); 
			return;
		}
		window.location.href="https://checkout.ule.com/payment/pay/presell-pay-select!index.do?orderNo="+escId+"&presellType="+type;
 	}
	function unsubscribe(obj,dloId,prdId){
		$(obj).attr("disabled",true);
		if(dloId==null||prdId==null){
			JEND.util.dialog.alert('活动太火爆啦，请稍后再试'); 
			return;
		}
		JEND.util.dialog.confirm({title:'操作确认', message:'您是否确定要退订此期刊？',
			    callback:function(){
				var urlStr="/myShoppingOrderWeb/unsubscribe.do";
		  		dataStr="dloId=" + dloId + "&prdId=" + prdId;
		  		var returnStr = $.ajax({
		  			type: "POST",
		  			url: urlStr,
		  			data:dataStr,
		  			cache: false,
		  			async: false
		  	    }).responseText;
		  	    JEND.util.dialog.alert(returnStr);
		  	     if(returnStr.indexOf("退订成功") > -1){
		         	$(obj).css("color", "gray").removeAttr("onclick");;
		        }
		  	}
		});
	}
	
	//于斯视频播放 采用jsonp，解决跨域问题
	function yusiTvPlay(escOrderid){
		$.ajax({
            url:"/myShoppingOrderWeb/yusiTvPlay.do",
            type:"get",
            data:{escId:escOrderid},
            dataType: "jsonp",
            jsonp: "jsonpCallback",
            cache: false,
	  		async: false,
            success:function(data){
            	if(data.code == "0001"){
            		alert("视频播放异常！");
            		return false;
            	}
            	document.getElementById("yusiOrderForm").action = data.returnMsg;
            	document.getElementById("yusiOrderForm").submit();
            },
            error: function(){
            	
            }         
         });
	}

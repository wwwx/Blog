							setTimeout(function(){ JEND.page.showLoading('正在刷新页面……'); }, 500);
							//$('#orderList').submit();
							//search();
							$('#orderDetail').submit();
						});
					}
				});
       // document.getElementById("orderDetail").action = "/myShoppingOrderWeb/cancelBuyerOrder.do";
       // document.getElementById("orderDetail").submit();
	}
